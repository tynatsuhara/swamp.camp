{
    "name": "swamp.camp",
    "version": "1.0.0",
    "description": "SWAMP CAMP",
    "scripts": {
        "versionize": "ts-node scripts/versionize.ts",
        "start": "npm run versionize && webpack serve --mode=development",
        "check": "CYCLE_CHECK=true webpack --mode=development",
        "build:prod": "npm run versionize && webpack --mode=production",
        "build:native": "cross-env IS_NATIVE=true npm run versionize && webpack --mode=production",
        "build:native:dev": "cross-env IS_NATIVE=true npm run versionize && webpack --mode=development",
        "format": "npx prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tynatsuhara/swamp.camp.git"
    },
    "author": "Tyler Natsuhara",
    "homepage": "https://swamp.camp",
    "devDependencies": {
        "circular-dependency-plugin": "^5.2.2",
        "cross-env": "^7.0.3",
        "prettier": "^2.7.1",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.10.0"
    },
    "dependencies": {
        "brigsby": "^0.8.15",
        "trystero": "^0.11.8"
    }
}
